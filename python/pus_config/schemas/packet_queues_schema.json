{
	"$schema": "http://json-schema.org/schema#",
	"id": "https://github.com/esrocos/TBD",
	"title": "On-board events",
	"description": "Events configuration for PUS packet queues",
	"type": "object",
	"properties": {
		"tcPacketQueueLength": {
			"type": "integer",
			"minimum": 0,
			"maximum": 2047
		},
		"tmPacketQueueLength": {
			"type": "integer",
			"minimum": 0,
			"maximum": 2047
		},
		"isTasteFlag": {
			"type": "integer",
			"enum": [
				0,
				1
			]
		}
	},
	"required": [
		"tcPacketQueueLength",
		"tmPacketQueueLength",
		"isTasteFlag"
	]
}
